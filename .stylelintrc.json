{
  "defaultSeverity": "error",
  "extends": [
    "stylelint-config-standard-scss",
    "stylelint-config-prettier-scss"
  ],
  "overrides": [
    {
      "files": "**/*.scss",
      "customSyntax": "postcss-scss"
    }
  ],
  "rules": {
    "scss/dollar-variable-empty-line-before": null,
    "selector-class-pattern": null
  }
}